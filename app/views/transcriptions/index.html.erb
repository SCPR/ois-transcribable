<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <ol class="breadcrumb">
            <li><%= link_to 'Home', controller: :home, action: :index %></li>
            <li>View Transcriptions</li>
        </ol>
        <h1>Transcriptions#index</h1>
        <li><%= link_to "Download All Transcriptions", transcriptions_path(format: :csv), class: "btn btn-info" %></li>
        <hr></hr>
        <ul>
            <% @transcriptions.each do |transcription| %>
                <li><h6><%= transcription.district_attorney_file_number %> (Filed by <%= transcription.email %>)</h6></li>
                <ul class="list-inline">
                    <li><%= link_to "View", transcription_path(transcription), class: "btn btn-info" %></li>
                    <li><%= link_to "Edit", edit_transcription_path(transcription), class: "btn btn-success" %></li>
                    <li><%= link_to "Delete", transcription_path(transcription), class: "btn btn-danger", method: :delete, data: {confirm: "are you sure?"} %></li>
                </ul>
                <hr></hr>
            <% end %>
        </ul>
    </div>
</div>
