<!DOCTYPE html>

<!-- begin doc -->
<html lang="en">

    <!-- begin header -->
    <head>

        <!-- begin meta -->
        <title><%= yield :title %></title>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
        <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.5, user-scalable=yes" />
        <meta name="apple-mobile-web-app-capable" content="yes" />
        <meta name="apple-mobile-web-app-status-bar-style" content="black" />
        <meta name="format-detection" content="telephone=no"/>
        <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
        <meta name="description" content="#" />
        <meta name="keywords" content="#" />
        <meta name="author" content="89.3 KPCC" />
        <meta property="og:title" content="#" />
        <meta property="og:site_name" content="KPCC" />
        <meta property="og:type" content="website" />
        <meta property="og:image" content="#" />
        <meta property="og:image:width" content="1024" />
        <meta property="og:image:height" content="683" />
        <meta property="og:url" content="#" />
        <meta property="og:description" content="#" />
        <meta name="twitter:card" content="summary"/>
        <meta name="twitter:url" content="#"/>
        <meta name="twitter:domain" content="http://projects.scpr.org/"/>
        <meta name="twitter:site" content="KPCC"/>
        <meta name="twitter:title" content="#"/>
        <meta name="twitter:description" content="#"/>
        <meta name="twitter:image:src" content="#">
        <link rel="shortcut icon" href="http://media.scpr.org/favicon.ico" type="image/x-icon" />
        <link rel="apple-touch-icon" href="http://media.scpr.org/assets/images/icon.png">
        <!-- end meta -->

        <!-- begin css -->
        <link href="http://projects.scpr.org/applications/monthly-water-use/static/monthly_water_reports/css/reset.css" media="screen" rel="stylesheet" type="text/css" />
        <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.0.0/css/bootstrap.min.css" rel="stylesheet" />
        <link href="http://projects.scpr.org/applications/monthly-water-use/static/monthly_water_reports/css/main-foundation-style-v3.css" rel="stylesheet" type="text/css" media="all" />
        <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
        <%= yield :css %>
        <!-- end css -->

        <!-- begin base scripts -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
        <script type="text/javascript" src="//use.typekit.net/vtu6xsq.js"></script>
        <script type="text/javascript">try{Typekit.load();}catch(e){}</script>

        <script src="//s3.amazonaws.com/s3.documentcloud.org/viewer/loader.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/backbone.js/1.2.1/backbone-min.js"></script>
        <!-- end base scripts -->

        <%= csrf_meta_tags %>

    </head>
    <!-- end header -->

    <!-- begin body -->
    <body class="minimal">
        <div class="kpcc-header">
            <header>
                <nav class="navbar navbar-default" role="navigation">
                    <div class="container data-container">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                            <h3 id="site-title" ><a href="http://www.scpr.org/">KPCC 89.3</a></h3>
                        </div>
                        <div class="data-share navbar-collapse collapse">
                            <h2 id="document-title" class="text-right"></h2>
                        </div>
                    </div>
                </nav>
            </header>
        </div>
        <div class="container data-container">
            <%= yield %>
            <div class="row kpcc-footer">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <footer>
                        <ul class="list-inline">
                            <li><a href="http://scpr.org/terms/">Terms &amp; Conditions</a></li>
                            <li><a href="http://scpr.org/privacy/">Privacy</a></li>
                            <li><a href="http://scpr.org/web_site_feedback">Website Feedback</a></li>
                            <li><a href="http://scpr.org/">Copyright 2013 &copy; Southern California Public Radio</a></li>
                        </ul>
                    </footer>
                </div>
            </div>
        </div>
        <!-- end main container -->

        <!-- begin scripts -->
        <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
        <%= yield :javascripts %>

    </body>
    <!-- end body -->

</html>
<!-- end document -->
